@model OnSales.Web.Data.Entities.TaxesType

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar</h1>

<h4>Tipo Impuesto</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="NameTaxesType" class="control-label"></label>
                <input asp-for="NameTaxesType" class="form-control" />
                <span asp-validation-for="NameTaxesType" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-outline-info">Listado</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
